{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "nelt",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "nelt_tags"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          },
          {
            "type": "SYMBOL",
            "name": "nelt_comment"
          }
        ]
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\{]"
            },
            {
              "type": "PATTERN",
              "value": "\\{[^\\#\\%]"
            },
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "STRING",
              "value": "# "
            }
          ]
        }
      }
    },
    "nelt_tags": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "nelt_expr"
        },
        {
          "type": "SYMBOL",
          "name": "nelt_val"
        }
      ]
    },
    "nelt_expr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{#"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^#]+"
                },
                {
                  "type": "STRING",
                  "value": "#"
                }
              ]
            }
          },
          "named": true,
          "value": "expr"
        },
        {
          "type": "STRING",
          "value": "#}"
        }
      ]
    },
    "nelt_val": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{{"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^}]+"
                    },
                    {
                      "type": "STRING",
                      "value": "}"
                    }
                  ]
                }
              },
              "named": true,
              "value": "expr"
            },
            {
              "type": "STRING",
              "value": "}}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{{-"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^}]+"
                    },
                    {
                      "type": "STRING",
                      "value": "}"
                    }
                  ]
                }
              },
              "named": true,
              "value": "expr"
            },
            {
              "type": "STRING",
              "value": "}}"
            }
          ]
        }
      ]
    },
    "nelt_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{["
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\\]]+"
                },
                {
                  "type": "STRING",
                  "value": "]"
                }
              ]
            }
          },
          "named": true,
          "value": "comment"
        },
        {
          "type": "STRING",
          "value": "]}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}